{
  "name": "revault",
  "version": "0.4.3",
  "description": "Yet another state management library",
  "main": "lib/revault.js",
  "module": "lib/revault.es.js",
  "repository": "https://github.com/kylealwyn/revault",
  "author": "Kyle Alwyn <kjalwyn@gmail.com>",
  "license": "MIT",
  "private": false,
  "files": [
    "lib",
    "logger.js",
    "README.md"
  ],
  "scripts": {
    "precommit": "lint-staged",
    "bump": "standard-version",
    "format": "prettier --write {src,example}/**/*.js",
    "test": "jest",
    "build": "npm-run-all clean rollup",
    "clean": "rimraf dist",
    "rollup": "rollup -c",
    "release": "npm-run-all test build bump && git push --follow-tags origin master && npm publish",
    "example": "parcel example/index.html"
  },
  "dependencies": {
    "create-react-context": "^0.2.2",
    "deep-object-diff": "^1.1.0"
  },
  "peerDependencies": {
    "prop-types": "^15.6.1",
    "react": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.2.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "enzyme-to-json": "^3.3.4",
    "husky": "^0.14.3",
    "immer": "^1.3.1",
    "jest": "^23.3.0",
    "lint-staged": "^7.2.0",
    "npm-run-all": "^4.1.3",
    "parcel-bundler": "^1.9.4",
    "prettier": "^1.13.7",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-test-renderer": "^16.4.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-filesize": "^2.0.0",
    "standard-version": "^4.4.0"
  },
  "lint-staged": {
    "*.{js}": [
      "prettier --write",
      "git add"
    ]
  }
}
